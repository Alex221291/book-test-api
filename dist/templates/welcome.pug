doctype
html
	head
		style(type="text/css").
			@import url('https://fonts.googleapis.com/css2?family=Ubuntu:wght@400;700&display=swap');
			#table {
				width: 100%;
				max-width: 768px;
				font-family: 'Ubuntu', Arial, sans-serif;
				color: #1A344B;
			}
			#header {
				padding: 1rem;
				background: #FFF;
				width: 100%;
				color: #FFF;
				font-size: 28px;
				text-align: center;
			}
			#header img {
				max-width: 230px;
			}
			#title {
				padding: 1rem;
				text-align: center;
				font-size: 36px;
				font-weight: bold;
			}
			#content {
				font-size: 15px;
				padding: 1rem
			}
			#button {
				dislay: table;
				background: #0085FF;
				padding: .7rem 1rem;
				color: #fff;
				text-decoration: none;
				border-radius: 8px;
			}
			#text {
				line-height: 24px;
				margin-bottom: 28px;
			}
			#small {
				padding-top: 14px;
				font-size: 13px;
				color: #828282;
			}
	body
		table#table
			tbody
				tr
					td#header
						img(src='data:image/png;base64,')
				tr
					td#title= title
				tr
					td#content
						p#text= description
						a#button(href=link)= linkText
						p#small= footer
